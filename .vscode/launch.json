    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
        ,
        {
            "name": "Pipeline",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pipeline.py",
            "args": ["llama3"],
            "console": "integratedTerminal"
        },
        {
            "name": "NLU evaluation",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/nlu_evaluation.py",
            "args": ["llama3"],
            "console": "integratedTerminal"
        }
    ]
}